<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.slc.entities">
	<class name="Journey" table="JOURNEY">
		<cache usage="read-write" />
		<id name="journeyId" column="JOURNEYID">
			<generator class="increment" />
		</id>
		<property name="source" column="SOURCE" />
		<property name="destination" column="DESTINATION" />
		<property name="dateOfJourney" column="DATEOFJOURNEY" />
	</class>
</hibernate-mapping>