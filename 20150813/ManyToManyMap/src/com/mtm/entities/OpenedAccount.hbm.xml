<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mtm.entities">
	<class name="OpenedAccount" table="OPENED_ACCOUNT">
		<id name="accountNo" column="ACCOUNT_NO">
			<generator class="increment" />
		</id>
		<property name="type" column="TYPE" />
		<property name="openedDate" column="OPENED_DT" />
		<property name="balance" column="BALANCE" />
		<property name="regMobile" column="REG_MOBILE" />
		<property name="regEmail" column="REG_EMAIL" />
		<property name="status" column="STATUS" />
	</class>
</hibernate-mapping>