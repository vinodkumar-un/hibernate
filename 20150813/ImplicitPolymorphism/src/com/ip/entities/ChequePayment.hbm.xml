<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.ip.entities">
	<class name="ChequePayment" table="CHEQUE_PAYMENT">
		<id name="paymentId" column="PAYMENT_ID">
			<generator class="increment" />
		</id>
		<property name="merchant" column="MERCHANT" />
		<property name="amount" column="AMOUNT" />
		<property name="chequeNo" column="CHEQUE_NO" />
		<property name="bank" column="BANK_NM" />
		<property name="issuedDate" column="ISS_DT" />
	</class>
</hibernate-mapping>